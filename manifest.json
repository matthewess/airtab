{
  "manifest_version": 2,
  "name": "Airtab",
  "version": "0",

  "key": "",

  "description": "Cloud-synced tab freezing/saving.",
  "icons": {
    "16": "img/cloud-16.png",
    "48": "img/cloud-48.png",
    "128": "img/cloud-128.png"
  },

  "browser_action": {
    "default_icon": {
      "16": "img/cloud-16.png",
      "24": "img/cloud-24.png",
      "32": "img/cloud-32.png"
    },
    "default_title": "Airtab"
  },

  "background": {
    "scripts": ["js/background.js"]
  },

  "permissions": [
    "tabs",
    "identity",
    "storage"
  ],
  "oauth2": {
    "client_id": "587428467193-ipb725b77jeaso5sa14q5ml0mton1t5p.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/spreadsheets"
    ]
  }
}